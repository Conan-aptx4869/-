@import "../../common/common.wxss";

.feedForm {
        display: block;
}

.feedForm textarea {
        width: 630rpx;
        height: 255rpx;
        border: 1px solid #e2e2e2;
        border-radius: 12rpx;
        margin: 30rpx auto;
        background: #fff;
        padding: 30rpx;
}

.feedForm input {
        height: 90rpx;
        width: 630rpx;
        margin: auto;
        background: #fff;
        border-radius: 12rpx;
        box-sizing: border-box;
        padding-left: 30rpx;
        border: 1px solid #e2e2e2;
}
.feedForm button{
        height: 90rpx;
        width: 625rpx;
        margin:30rpx  auto;
        background: #ff5722;
        color: #fff;
        border-radius: 12rpx;
   
       
}
