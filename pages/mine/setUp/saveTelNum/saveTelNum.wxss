@import"../../../common/common.wxss";
.saveForm {
        width: 670rpx;
        padding-top: 30rpx;
}

.saveForm .item{
        display: flex;
        flex-direction: row;
        justify-content: space-between;
        height: 85rpx;        
        margin-bottom: 30rpx;
        border:1px solid #e9e9e9;
}
.saveForm .item input{
        border: none;
        height: 100%;
}
.saveForm .item label{
        width: 150rpx;
        height: 100%; 
        line-height: 85rpx;
        display: table-cell;
        vertical-align: middle;
        text-align: center;
        background: #fff;
}
.tel{
        width: 525rpx;
}
.code{
        width: 295rpx;
}
.count{
        width: 245rpx;
        border-left: 1px solid #e9e9e9 !important;
        height: 100%;
        line-height: 85rpx;
        background: #fff;
        text-align: center;
        color: #ff5722;
        padding-left: 0 !important;
}