/*自定义弹窗*/

.modal-mask {
        width: 100%;
        height: 100%;
        position: fixed;
        top: 0;
        left: 0;
        background: #000;
        opacity: 0.8;
        overflow: hidden;
        z-index: 9000;
        color: #fff;
}

.modal-dialog {
        width: 415rpx;
        height: 460rpx;
        overflow: hidden;
        position: fixed;
        top: 0;
        left: 0;
        bottom: 0;
        right: 0;
        z-index: 9999;
        background: #f9f9f9;
        margin: auto;
        text-align: center;
}

.modal-dialog  .photo {
        width: 200rpx;
        height: 200rpx;
}

.modal-dialog  .text1 {
        margin-top: 30rpx;
}

.modal-dialog  .text2 {
        margin: 10rpx;
}

.modal-dialog  .text3 {
        font-size: 24rpx;
}

.modal-footer {
        position: absolute;
        bottom: 30rpx;
        left: 0;
        right: 0;
}

.btn-confirm {
        background: #fe5722;
        color: #fff;
        text-align: center;
        border-radius: 5rpx;
        margin: auto;
        width: 130rpx;
        height: 45rpx;
        line-height: 45rpx;
        font-size: 24rpx;
}

/*自定义弹窗 */
