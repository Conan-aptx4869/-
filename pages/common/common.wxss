page {
  font-size: 28rpx;
  overflow-x: hidden;
  position: relative;
  background: #f3f3f3;
  padding-bottom: 90rpx;
}

view, input, label, text, button, textarea {
  box-sizing: border-box;
}

.container {
  padding: 20rpx;
}

.clearfix:after {
  content: "";
  display: table;
  height: 0;
  visibility: both;
  clear: both;
}

button::after {
  border: none;
}

input {
  outline: none;
  border: none;
  list-style: none;
}

.noBody {
  font-size: 32rpx;
  color: #333;
  text-align: center;
  margin: 40rpx 0 20rpx;
}

.ellips {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.mask {
  width: 100%;
  height: 100%;
  background-color: #000;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 3;
  opacity: 0.7;
  display: block;
}

.show {
  display: block;
}

.show_inline {
  display: inline-block;
}

.hide {
  display: none !important;
}

.goods_listx .item {
  padding: 25rpx 30rpx;
  background: #fff;
  margin-bottom: 10rpx;
}

.goods_listx .item  .photo {
  width: 170rpx;
  height: 170rpx;
  border: 1px solid #f2f2f2;
  vertical-align: top;
  margin-right: 20rpx;
}

.goods_listx .item  .info {
  width: 500rpx;
  display: inline-block;
}

.goods_listx .item .info .content {
  color: #666;
  margin: 10rpx 0;
}

.goods_listx .item .info .price {
  color: #ff8061;
  font-size: 26rpx;
  font-weight: bold;
}

.goods_listy {
  background: #fff;
  display: flex;
  flex-flow: row wrap;
  border-top: 1px solid #f1f1f1;
  padding-bottom: 60rpx;
}

.goods_listy>.item {
  width: 375rpx;
  padding: 20rpx 40rpx;
  border-bottom: 1px solid #f1f1f1;
  position: relative;
}

.goods_listy>.item:nth-child(2n+1) {
  border-right: 1px solid #f1f1f1;
}

.goods_listy>.item .photo {
  width: 295rpx;
  height: 295rpx;
  margin-bottom: 25rpx;
}

.goods_listy>.item .price {
  margin: 15rpx auto;
}

.goods_listy>.item .old_price {
  text-decoration: line-through;
  margin-left: 20rpx;
  color: #666;
}

.goods_listy>.item .discount {
  color: #ff8061;
}

.goods_listy>.item .discount text {
  font-size: 24rpx;
}

.goods_listy .item .fab {
  width: 30rpx;
  height: 28rpx;
  position: absolute;
  right: 50rpx;
  top: 40rpx;
}

.section_title {
  width: 100%;
  height: 200rpx;
  text-align: center;
  background: #fff;
  font: normal 40rpx "黑体";
  display: flex;
  flex-direction: column;
  justify-content: center;
  /* box-shadow: 0 5px 5px #ddd inset; *//* align-items: center; */
}

.section_title text {
  display: block;
  margin-top: 20rpx;
  font-size: 32rpx;
  font-weight: normal;
}

.order_num {
  border-bottom: 1px solid #ccc;
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 80rpx;
  align-items: center;
}

.order_num text {
  color: #000;
}

.order_num .detele {
  width: 22rpx;
  height: 27rpx;
}

.state {
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 125rpx;
  border-bottom: 1px solid #ccc;
  margin-bottom: 20rpx;
}

.state text {
  color: #ff5722;
}

.state .button {
  display: inline-block;
  float: right;
}

.state .cancel {
  background: none !important;
  color: #999 !important;
  border: 1px solid #ccc;
}

.state_btn {
  width: 150rpx;
  height: 55rpx;
  text-align: center;
  line-height: 55rpx;
  background: #ff5722;
  color: #fff;
  border-radius: 10rpx;
  display: inline-block;
  margin: 0 8rpx;
  vertical-align: top;
}

.goods_info {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 30rpx 20rpx;
}

.goods_info .photo {
  width: 150rpx;
  height: 150rpx;
}

.goods_info .text {
  width: 500rpx;
}

.goods_info .name {
  color: #000;
  margin-bottom: 10rpx;
}

.goods_info  .price {
  float: right;
  color: #ff5722;
}

.goods_info .attr {
  margin-top: 15rpx;
}

.saveForm {
  display: block;
  margin: auto;
}

.saveForm input {
  display: block;
  height: 85rpx;
  background: #fff;
  padding-left: 30rpx;
  font-size: 30rpx;
  border: 1px solid #e9e9e9;
}

.saveForm button {
  background: #ff5722;
  width: 630rpx;
  height: 90rpx;
  border-radius: 12rpx;
  color: #fff;
}

/*分类列表  */

.classic_list {
  overflow-x: scroll;
  white-space: nowrap;
  padding: 20rpx 0 40rpx;
  margin: 0 auto;
}

.classic_list .item {
  width: 180rpx;
  text-align: center;
  display: inline-block;
  white-space: normal;
  margin-left: 30rpx;
  vertical-align: top;
}

.classic_list .item image {
  width: 100%;
  height: 200rpx;
  box-sizing: border-box;
}

.classic_list .item image.selected {
  border: 1px solid orangered;
}

/*分类列表  */

/*排序  */

.sort {
  background: white;
  width: 100%;
  height: 85rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  line-height: 85rpx;
}

.sort  .sort_item {
  width: 100rpx;
  text-align: center;
}

.sort  .sort_item::after {
  content: "";
  width: 15rpx;
  height: 15rpx;
  display: inline-block;
  background-image: url(data:image/png;base64,);
  background-size: contain;
  background-position: top center;
  background-repeat: no-repeat;
}

.sort .current {
  border-bottom: 2px solid #ff5722;
  color: #ff5722;
}

.sort .current::after {
  background-image: url(data:image/png;base64,);
}

.both {
  background: white;
  display: block;
}

.both .both_sort {
  padding: 20rpx;
  background: #fff;
}

.both .both_sort view {
  height: 75rpx;
  line-height: 75rpx;
}

.both .both_sort  view:first-child {
  border-bottom: 1px solid #ccc;
}

/*排序  */

/*优惠券  */

.coupon_list .coupon_item {
  width: 710rpx;
  margin: 20rpx auto;
  background: #fff;
  padding-top: 45rpx;
  border-radius: 18rpx;
}

.coupon_list .coupon_item .money {
  width: 245rpx;
  padding-left: 40rpx;
  display: inline-block;
  vertical-align: top;
}

.coupon_list .coupon_item .money .num {
  font-size: 75rpx;
  font-weight: bold;
  vertical-align: -35rpx;
}

.coupon_list .coupon_item .money .symbol {
  font-size: 32rpx;
}

.coupon_list .coupon_item .money .condition {
  margin: 24rpx auto 30rpx;
}

.coupon_list .coupon_item   .info {
  width: 465rpx;
  display: inline-block;
}

.coupon_list .coupon_item   .info .content {
  width: 375rpx;
  font-size: 25rpx;
  display: inline-block;
  vertical-align: top;
}

.coupon_list .coupon_item   .info .icon {
  color: #fff;
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
  margin-right: 15rpx;
  font-size: 20rpx;
}

.coupon_list .coupon_item   .info .time {
  font-size: 22rpx;
  margin-top: 40rpx;
}

.coupon_list .coupon_item  .explain {
  height: 55rpx;
  line-height: 55rpx;
  text-indent: 20rpx;
  border-top: 1px dashed transparent;
  font-size: 22rpx;
  color: #999;
}

.coupon_list .ava {
  border-top: 15rpx solid #ff5722;
}

.coupon_list .ava .money {
  color: #ff5722;
}

.coupon_list .ava .icon {
  background: #ff5722;
}

.coupon_list .ava .explain {
  border-color: #ff5722;
}

/*不可用优惠券  */

.coupon_list .unava {
  border-top: 15rpx solid #e2e2e2;
}

.coupon_list .unava .money {
  color: #e2e2e2;
}

.coupon_list .unava .icon {
  background: #e2e2e2;
}

.coupon_list .unava .explain {
  border-color: #e2e2e2;
}

/*评论列表  */

.comment_list {
}

.comment_list .item {
  padding: 25rpx;
  margin-bottom: 20rpx;
  background: #fff;
}

.comment_list .item  .head {
}

.comment_list .item  .head .avatar {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  vertical-align: middle;
}

.comment_list .item  .head  .name {
  margin-right: 15rpx;
  vertical-align: middle;
}

.comment_list .item  .head  .date {
  font-size: 25rpx;
  color: #999;
  vertical-align: middle;
}

.comment_list .item  .head .score {
  float: right;
}

.comment_list .item  .head .score image {
  width: 25rpx;
  height: 25rpx;
  margin-right: 5rpx;
}

.comment_list .item  .text {
  margin-top: 25rpx;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}

.comment_list .item .photo {
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
}

.comment_list .item .photo image {
  width: 200rpx;
  height: 200rpx;
  border: 1px solid #eaeaea;
  border-radius: 12rpx;
  margin-bottom: 30rpx;
}

/*评论列表  */

/*自定义弹窗*/

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.8;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 415rpx;
  height: 460rpx;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: auto;
  text-align: center;
}

.modal-dialog  .photo {
  width: 200rpx;
  height: 200rpx;
}

.modal-dialog  .text1 {
  margin-top: 30rpx;
}

.modal-dialog  .text2 {
  margin: 10rpx;
}

.modal-dialog  .text3 {
  font-size: 24rpx;
}

.modal-footer {
  position: absolute;
  bottom: 30rpx;
  left: 0;
  right: 0;
}

.btn-confirm {
  background: #fe5722;
  color: #fff;
  text-align: center;
  border-radius: 5rpx;
  margin: auto;
  width: 130rpx;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 24rpx;
}

/*自定义弹窗 */
